<?xml version="1.0"?>
<odoo>
    <record id="view_res_company_fields_init" model="ir.ui.view">
        <field name="name">company.fields.init</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='email']" position="after">
                <field name="cin_emp"/>
                <field name="uan_emp"/>
                <field name="startup_emp"/>
            </xpath>
        </field>
    </record>
</odoo>